{
  "name": "csaba-nagy/database-adapter-php",
  "type": "library",
  "description": "Database Adapter for PHP API services",
  "version": "0.1.0",
  "license": "MIT",
  "authors": [
    {
      "name": "Csaba Nagy",
      "email": "n.csaba1991@gmail.com"
    }
  ],
  "scripts": {
    "test": "phpunit"
  },
  "require": {
    "php": "^8.0.2"
  },
  "require-dev": {
    "fakerphp/faker": "^1.21.0",
    "phpunit/phpunit": "^9.5.28"
  },
  "autoload": {
    "psr-4": {
      "DatabaseAdapterPhp\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
